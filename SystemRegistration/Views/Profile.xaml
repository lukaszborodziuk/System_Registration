<Window x:Class="SystemRegistration.Profile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SystemRegistration"
        mc:Ignorable="d"
        Title="Profile" MinHeight="150" MinWidth="300" Width="410" Height="150" Background="#FF22242B" Loaded="Window_Loaded" Icon="/SystemRegistration;component/Assets/logo.ico" >
    <StackPanel Grid.Column="2" Grid.Row="2" Height="150" Width="400">
        <Label Content="Your name" Height="30" Width="70" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,10,0,0" Foreground="White"></Label>
        <TextBlock Name="nameRead" Width="200" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-30,0,0" Background="#FF343949" FontSize="20" Foreground="White"></TextBlock>
        <Label Content="Type Account" Height="30" Width="90" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,10,0,0" Foreground="White"></Label>
        <TextBlock Name="accountRead" Width="200" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-30,0,0" Background="#FF343949" FontSize="20" Foreground="White"></TextBlock>
    </StackPanel>
</Window>
